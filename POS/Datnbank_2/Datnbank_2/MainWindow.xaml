<Window x:Class="Datnbank_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Datnbank_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Titel" HorizontalAlignment="Left" Height="23" Margin="54,46,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Year" HorizontalAlignment="Left" Height="23" Margin="54,98,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="apply" Content="Button" HorizontalAlignment="Left" Margin="54,149,0,0" VerticalAlignment="Top" Width="120" Height="27" Click="apply_Click"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="54,233,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <ListBox x:Name="listbox" HorizontalAlignment="Left" Height="246" Margin="509,46,0,0" VerticalAlignment="Top" Width="199"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="54,281,0,0" VerticalAlignment="Top" Width="120" Height="27"/>

    </Grid>
</Window>
